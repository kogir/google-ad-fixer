{
  "manifest_version": 2,

  "name": "Google Ad Remover",
  "version": "0.1.0",
  "description": "Hides Google Search ads",

  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.google.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}
